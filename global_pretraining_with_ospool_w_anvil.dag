SUBMIT-DESCRIPTION annex_helper.sub {
    universe = local
    executable = /home/ian.ross/MLDAG/.venv/bin/python
    arguments = annex_helper.py watch --interval 60
    queue
}
SERVICE annex_helper annex_helper.sub
SUBMIT-DESCRIPTION ospool_pretrain.sub {
	universe = container
	container_image = osdf:///ospool/ap40/data/ian.ross/metl_global.sif 

	request_disk = 80GB
	request_memory = 65536
	request_cpus = 4
	request_gpus = 1
	#gpus_minimum_memory = 8192
	#gpus_minimum_capability = 7.5

	+is_resumable = true
	+JobDurationCategory = "Long"
    +ProjectName = "PATh_MLSupplement"

	executable = /bin/bash
	transfer_executable = false
	arguments = pretrain.sh $(epoch) $(run_uuid) $(random_seed)

	preserve_relative_paths = true
	transfer_input_files = pretrain.sh, osdf:///ospool/ap40/data/ian.ross/processed-global.tar.gz
	if defined continue_from_checkpoint 
	    transfer_input_files = $(transfer_input_files), output/training_logs/$(run_uuid)
	endif
	transfer_output_files = output
	should_transfer_files = YES
	when_to_transfer_output = ON_EXIT_OR_EVICT

	output = $(run_uuid)/$(epoch)_$(CLUSTERID).out
	error = $(run_uuid)/$(epoch)_$(CLUSTERID).err
	stream_output = true
	stream_error = true
	log = metl.log

	TARGET.GLIDEIN_ResourceName == "ospool"
	environment = "WANDB_API_KEY=0713980c153b1601f06370cb13f67b1182ff0221"
	queue
}
